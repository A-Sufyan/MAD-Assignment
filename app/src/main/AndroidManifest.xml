<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sg.edu.np.worthornot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MADAssignment">
        <activity
            android:name="sg.edu.np.worthornot.adviceActivityHomepage"
            android:label="@string/title_activity_advice_homepage"
            android:theme="@style/Theme.MADAssignment.NoActionBar"></activity>
        <activity android:name="sg.edu.np.worthornot.adviceActivity" />
        <activity android:name="sg.edu.np.worthornot.CompareActivity" />
        <activity android:name="sg.edu.np.worthornot.ShoppingListActivity" />
        <activity android:name="sg.edu.np.worthornot.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>